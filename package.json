{
  "name": "npm-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 start ./pm2.config.cjs",
    "deploy": "node scripts/upload-server.js",
    "dev": "webpack serve --env goal=local --env ahg=testVal",
    "serve": "node serve-config/serve.js",
    "watch": "webpack --watch",
    "build": "node scripts/predev.js -build && webpack --mode=production --node-env=production",
    "build:lib": "node scripts/build-lib.js",
    "minSvg": "node packages/basecmp/icon/scripts/min-svg.cjs",
    "gene-icon": "node packages/basecmp/icon/scripts/gene-icon-bin.cjs svgDir=./packages/basecmp/icon/assets iconPath=./packages/basecmp/icon/src/icon.js",
    "build:icon": "node scripts/build-icon.js",
    "edit:pkg": "node scripts/edit-package.js",
    "link:all": "node scripts/link-local.js",
    "update:registry": "node scripts/update-registry.js",
    "create:comp": "node scripts/create.js",
    "prepare": "husky install",
    "lint": "npx eslint --fix --cache --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "npx prettier --write ."
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue}": "eslint --cache --fix",
    "packages/**/*.{js,jsx,vue}": "eslint --cache --fix"
  },
  "browserslist": {
    "development": [
      "> 1%",
      "last 2 versions",
      "not dead"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not dead"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "core-js": "3.24.1",
    "highlight.js": "11.7.0",
    "quill": "1.3.7",
    "resize-observer-polyfill": "1.5.1",
    "throttle-debounce": "3.0.1",
    "vue": "3.3.4",
    "vue-router": "4.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.20.7",
    "@babel/plugin-transform-react-jsx": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/compiler-sfc": "3.3.4",
    "axios": "0.27.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "9.1.0",
    "body-parser": "1.20.0",
    "css-loader": "5.2.7",
    "css-minimizer-webpack-plugin": "4.2.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.8.0",
    "file-loader": "6.2.0",
    "font-carrier": "0.3.1",
    "github-markdown-css": "5.1.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.0",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "lint-staged": "13.0.3",
    "mini-css-extract-plugin": "2.7.1",
    "multiparty": "4.2.3",
    "prettier": "2.7.1",
    "raw-loader": "4.0.2",
    "style-loader": "3.3.1",
    "style-resources-loader": "1.4.1",
    "svg-url-loader": "7.1.1",
    "svgo": "2.8.0",
    "url-loader": "4.1.1",
    "vue-loader": "17.2.2",
    "vue-markdown-loader": "2.5.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-middleware": "6.0.0",
    "webpack-dev-server": "4.11.1",
    "ws": "8.12.0"
  }
}
